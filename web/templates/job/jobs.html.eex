<div class="jumbotron">
  <h2>Jobs on the <%= @cluster %> cluster</h2>
</div>

<div class="container">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Job Name</th>
      </tr>
    </thead>
    <tbody>
      <%= for job <- @jobs do %>
      <tr>
        <td><a href="/jobs/<%= job %>" ><%= job %></a></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
